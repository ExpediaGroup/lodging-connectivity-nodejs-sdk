{
  "name": "@expediagroup/lodging-connectivity-sdk",
  "version": "1.0.0-beta.8",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./supply": {
      "require": "./dist/supply/index.cjs.js",
      "import": "./dist/supply/index.esm.js",
      "types": "./dist/supply/index.d.ts"
    },
    "./sandbox": {
      "require": "./dist/sandbox/index.cjs.js",
      "import": "./dist/sandbox/index.esm.js",
      "types": "./dist/sandbox/index.d.ts"
    },
    "./payment": {
      "require": "./dist/payment/index.cjs.js",
      "import": "./dist/payment/index.esm.js",
      "types": "./dist/payment/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf dist",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "codegen": "graphql-codegen",
    "publish:prod": "pnpm clean && pnpm build && pnpm publish --access public --no-git-checks",
    "publish:beta": "pnpm clean && pnpm build && pnpm publish --access public --tag beta --no-git-checks"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
